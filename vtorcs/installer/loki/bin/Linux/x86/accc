prefix=@prefix@
exec_prefix=${prefix}

BINDIR=${exec_prefix}
LIBDIR=${exec_prefix}/lib
DATADIR=${prefix}/share
VARDIR=${prefix}/var

export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$LIBDIR/lib

$LIBDIR/accc-bin $*

rm -f temp.objf

exit 0
