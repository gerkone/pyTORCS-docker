		<attnum name="ride height" unit="mm" min="70" max="120" val="100"/>
		<attnum name="toe" unit="deg" min="-1" max="1" val="0"/>
		<attnum name="camber" min="-5" max="-3" unit="deg" val="-5"/>
		
		<!-- Adherence -->
		<attnum name="stiffness" val="25.0"/>
		<attnum name="dynamic friction" unit="%" val="80"/>
		<attnum name="mu" min="0.05" max="1.6" val="1.6"/>
	</section>
	
	<section name="Rear Right Wheel">
		<attnum name="ypos" unit="m" val="-0.8"/>
		<attnum name="rim diameter" unit="in" min="16" max="18" val="18.0"/>
		<attnum name="tire width" unit="mm" min="135" max="335" val="330"/>
		<attnum name="tire height-width ratio" min="0.3" max="0.8" val=".3"/>
		<attnum name="inertia" unit="kg.m2" val="1.2200"/>
		
		<!-- initial ride height -->
		<attnum name="ride height" unit="mm" min="70" max="120" val="100"/>
		<attnum name="toe" unit="deg" min="-1" max="1" val="0"/>
		<attnum name="camber" min="-5" max="-2" unit="deg" val="-3"/>
		
		<!-- Adherence -->
		<attnum name="stiffness" val="25.0"/>
		<attnum name="dynamic friction" unit="%" val="80"/>
		<attnum name="mu" min="0.05" max="1.6" val="1.6"/>
	</section>
	
	<section name="Rear Left Wheel">
		<attnum name="ypos" unit="m" val="0.8"/>
		<attnum name="rim diameter" unit="in" min="16" max="18" val="18.0"/>
		<attnum name="tire width" unit="mm" min="135" max="335" val="330"/>
		<attnum name="tire height-width ratio" min="0.3" max="0.8" val=".3"/>
		<attnum name="inertia" unit="kg.m2" val="1.2200"/>
		
		<!-- initial ride height -->
		<attnum name="ride height" unit="mm" min="70" max="120" val="100"/>
		<attnum name="toe" unit="deg" min="-1" max="1" val="0"/>
		<attnum name="camber" min="-5" max="-2" unit="deg" val="-3"/>
		
		<!-- Adherence -->
		<attnum name="stiffness" val="25.0"/>
		<attnum name="dynamic friction" unit="%" val="80"/>
		<attnum name="mu" min="0.05" max="1.6" val="1.6"/>
	</section>
	
	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
	</section>
	
	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
	</section>
	
	<section name="Front Right Suspension">
		<attnum name="spring" unit="lbs/in" min="500" max="10000" val="5500"/>
		<attnum name="suspension course" unit="m" min="0.2" max="0.3" val="0.2"/>
		<attnum name="bellcrank" min="1" max="1.5" val="1.2"/>
		<attnum name="packers" unit="mm" min="0" max="30" val="10"/>
		<attnum name="slow bump" unit="lbs/in/s" min="50" max="1000" val="400"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="50" max="1000" val="300"/>
		<attnum name="fast bump" unit="lbs/in/s" min="10" max="200" val="60"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="10" max="200" val="60"/>
	</section>
	
	<section name="Front Left Suspension">
		<attnum name="spring" unit="lbs/in" min="500" max="10000" val="5500"/>
		<attnum name="suspension course" unit="m" min="0.2" max="0.3" val="0.2"/>
		<attnum name="bellcrank" min="1" max="1.5" val="1.2"/>
		<attnum name="packers" unit="mm" min="0" max="30" val="10"/>
		<attnum name="slow bump" unit="lbs/in/s" min="50" max="1000" val="400"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="50" max="1000" val="300"/>
		<attnum name="fast bump" unit="lbs/in/s" min="10" max="200" val="60"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="10" max="200" val="60"/>
	</section>
	
	<section name="Rear Right Suspension">
		<attnum name="spring" unit="lbs/in" min="500" max="10000" val="5500"/>
		<attnum name="suspension course" unit="m" min="0.2" max="0.3" val="0.2"/>
		<attnum name="bellcrank" min="1" max="1.5" val="1.2"/>
		<attnum name="packers" unit="mm" min="0" max="30" val="0"/>
		<attnum name="slow bump" unit="lbs/in/s" min="50" max="1000" val="300"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="50" max="1000" val="400"/>
		<attnum name="fast bump" unit="lbs/in/s" min="10" max="200" val="60"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="10" max="200" val="60"/>
	</section>
	
	<section name="Rear Left Suspension">
		<attnum name="spring" unit="lbs/in" min="500" max="10000" val="5500"/>
		<attnum name="suspension course" unit="m" min="0.2" max="0.3" val="0.2"/>
		<attnum name="bellcrank" min="1" max="1.5" val="1.2"/>
		<attnum name="packers" unit="mm" min="0" max="30" val="0"/>
