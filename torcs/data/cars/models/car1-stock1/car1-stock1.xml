<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="car1-stock1" type="template">
	<section name="Driver">

		<!-- Position of the driver -->
		<attnum name="xpos" val="0.7" unit="m"/>
		<attnum name="ypos" val="0.0" unit="m"/>
		<attnum name="zpos" val="1.15" unit="m"/>
	</section>

	<section name="Sound">
		<attstr name="engine sample" val="ford1b.wav"/>
		<attnum name="rpm scale" val="1.25"/>
	</section>


	<section name="Graphic Objects">
		<attstr name="env" val="car1-stock1.acc"/>
		<attstr name="wheel texture" val="tex-wheel.rgb"/>
		<attstr name="shadow texture" val="shadow.rgb"/>
		<attstr name="tachometer texture" val="rpm8500.rgb"/>
		<attnum name="tachometer min value" unit="rpm" val="0"/>
		<attnum name="tachometer max value" unit="rpm" val="10000"/>
		<attstr name="speedometer texture" val="speed360.rgb"/>
		<attnum name="speedometer min value" unit="km/h" val="0"/>
		<attnum name="speedometer max value" unit="km/h" val="360"/>

		<attstr name="3d wheel basename" val="wheel"/>
		<attstr name="3d wheel directory" in="stock1-1,stock1-2,stock1-3" val="stock1-2"/>


		<section name="Ranges">
			<section name="1">
				<attnum name="threshold" val="0"/>
				<attstr name="car" val="car1-stock1.acc"/>
				<attstr name="wheels" val="yes"/>
			</section>
		</section>
		
		<section name="Light">
			<section name="1">
				<attstr name="type" val="brake2"/>
				<attnum name="xpos" val="-2.378"/>
				<attnum name="ypos" val="0.666"/>
				<attnum name="zpos" val="0.716"/>
				<attnum name="size" val="0.12"/>
			</section>

			<section name="2">
				<attstr name="type" val="brake2"/>
				<attnum name="xpos" val="-2.378"/>
				<attnum name="ypos" val="-0.666"/>
				<attnum name="zpos" val="0.716"/>
				<attnum name="size" val="0.12"/>
			</section>
		</section>

	</section>

	<section name="Car">
		<attstr name="category" val="stock1"/>
		<attnum name="body length" unit="m" val="4.90"/>
		<attnum name="body width" unit="m" val="1.92"/>
		<attnum name="body height" unit="m" val="1.26"/>

		<!-- collision bounding box -->
		<attnum name="overall length" unit="m" val="4.90"/>
		<attnum name="overall width" unit="m" val="1.92"/>
		<attnum name="GC height" unit="m" val="0.3"/>
		<attnum name="mass" unit="kg" val="1550.0"/>

		<!-- weight bias -->
		<attnum name="front-rear weight repartition" min="0.3" max="0.7" val="0.48"/>
		<attnum name="front right-left weight repartition" min="0.4" max="0.6" val="0.5"/>
		<attnum name="rear right-left weight repartition" min="0.4" max="0.6" val="0.5"/>

		<!-- used for inertia, smaller values indicate better mass centering -->
		<attnum name="mass repartition coefficient" min="0.5" max="5" val="0.8"/>
		<attnum name="fuel tank" unit="l" val="60.0"/>
		<attnum name="initial fuel" unit="l" min="1.0" max="60.0" val="60.0"/>
	</section>

	<section name="Exhaust">
	<!-- for flames -->
		<attnum name="power" val="1.5"/>
		<section name="1">
			<attnum name="xpos" val="-0.65"/>
			<attnum name="ypos" val="-1.00"/>
			<attnum name="zpos" val="0.12"/>
		</section>

		<section name="2">
			<attnum name="xpos" val="-0.65"/>
			<attnum name="ypos" val="1.00"/>
			<attnum name="zpos" val="0.12"/>
		</section>
	</section>

	<section name="Aerodynamics">
		<attnum name="Cx" val="0.38"/>
		<attnum name="front area" unit="m2" val="2.25"/>
		<attnum name="front Clift" val="0.25"/>
		<attnum name="rear Clift" val="0.55"/>
	</section>

	<section name="Front Wing">
		<attnum name="area" unit="m2" val="0.3"/>
		<attnum name="angle" min="0" max="30" unit="deg" val="15"/>
		<attnum name="xpos" unit="m" val="2.1"/>
		<attnum name="zpos" unit="m" val="0.04"/>
	</section>

	<section name="Rear Wing">
		<attnum name="area" unit="m2" val="0.35"/>
		<attnum name="angle" unit="deg" min="0" max="30" val="16"/>
		<attnum name="xpos" unit="m" val="-2.5"/>
		<attnum name="zpos" unit="m" val="1.05"/>
	</section>


	<!-- Same engine for every one -->
	<section name="Engine">
		<attnum name="revs maxi" unit="rpm" min="5000" max="10000" val="10000"/>
		<attnum name="revs limiter" unit="rpm" min="3000" max="10000" val="9000"/>
		<attnum name="tickover" unit="rpm" min="900" max="3000" val="1500"/>
		<attnum name="fuel cons factor" min="1.1" max="1.3" val="1.2"/>
		<attnum name="brake coefficient" val="0.2"/>
		
		<section name="data points">
			<section name="1">
				<attnum name="rpm" unit="rpm" val="0"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="2000.0" val="100"/>
			</section>

			<section name="2">
				<attnum name="rpm" unit="rpm" val="1000"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="1473.0" val="200"/>
			</section>

			<section name="3">
				<attnum name="rpm" unit="rpm" val="2000"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="1355.0" val="333"/>
			</section>

			<section name="4">
				<attnum name="rpm" unit="rpm" val="3000"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="1275.0" val="420"/>
			</section>

			<section name="5">
				<attnum name="rpm" unit="rpm" val="4000"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="1145.0" val="520"/>
			</section>

			<section name="6">
				<attnum name="rpm" unit="rpm" val="5000"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="1000.0" val="640"/>
			</section>

			<section name="7">
				<attnum name="rpm" unit="rpm" val="6000"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="884.0" val="670"/>
			</section>

			<section name="8">
				<attnum name="rpm" unit="rpm" val="7000"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="799.0" val="710"/>
			</section>

			<section name="9">
				<attnum name="rpm" unit="rpm" val="8000"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="735.0" val="690"/>
			</section>

			<section name="10">
				<attnum name="rpm" unit="rpm" val="9000"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="686.0" val="663"/>
			</section>

			<section name="11">
				<attnum name="rpm" unit="rpm" val="10000"/>
				<attnum name="Tq" unit="N.m" min="0.0" max="647.0" val="590"/>
			</section>

		</section>
	</section>

	<section name="Gearbox">
		<attnum name="shift time" val="0.2" unit="s"/>
		<section name="gears">
		<section name="r">
			<attnum name="ratio" min="-6" max="-3" val="-4.0"/>
			<attnum name="inertia" val="0.0037"/>
			<attnum name="efficiency" val="0.957"/>
		</section>

		<section name="1">
			<attnum name="ratio" min="0.5" max="5" val="3.82"/>
			<attnum name="inertia" val="0.003"/>
			<attnum name="efficiency" val="0.955"/>
		</section>

		<section name="2">
			<attnum name="ratio" min="0.5" max="5" val="2.15"/>
			<attnum name="inertia" val="0.0037"/>
			<attnum name="efficiency" val="0.957"/>
		</section>

		<section name="3">
			<attnum name="ratio" min="0.5" max="5" val="1.56"/>
			<attnum name="inertia" val="0.0048"/>
			<attnum name="efficiency" val="0.950"/>
		</section>

		<section name="4">
			<attnum name="ratio" min="0.5" max="5" val="1.21"/>
			<attnum name="inertia" val="0.0064"/>
			<attnum name="efficiency" val="0.983"/>
		</section>

		<section name="5">
			<attnum name="ratio" min="0.5" max="5" val="0.97"/>
			<attnum name="inertia" val="0.0107"/>
			<attnum name="efficiency" val="0.948"/>
		</section>

		<section name="6">
			<attnum name="ratio" min="0.5" max="5" val="0.8"/>
			<attnum name="inertia" val="0.0150"/>
			<attnum name="efficiency" val="0.940"/>
		</section>
		</section>
	</section>

	<section name="Steer">
		<attnum name="steer lock" unit="deg" min="5" max="45" val="21"/>
		<attnum name="max steer speed" min="1" max="360" unit="deg/s" val="360"/>
	</section>

	<section name="Drivetrain">
		<attstr name="type" val="RWD"/>
	</section>

	<section name="Rear Differential">
		<attstr name="type" in="LIMITED SLIP" val="LIMITED SLIP"/>
		<attnum name="inertia" unit="kg.m2" val="0.0488"/>
		<attnum name="ratio" min="1.0" max="10" val="4.5"/>
		<attnum name="efficiency" min=".9" max="1.0" val="0.9625"/>

		<attnum name="max slip bias" min="0.0" max="0.5" val="0.03"/>
		<attnum name="locking input torque" min="500" max="90000" val="5000"/>
		<attnum name="locking brake input torque" min="150" max="90000" val="1500"/>
	</section>

	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.66"/>
		<attnum name="max pressure" unit="kPa" min="5000" max="40000" val="25000"/>
	</section>

	<section name="Front Axle">
		<attnum name="xpos" unit="m" val="1.602"/>
		<attnum name="roll center height" unit="m" val="0.25"/>
	</section>

	<section name="Rear Axle">
		<attnum name="xpos" unit="m" val="-1.198"/>
		<attnum name="roll center height" unit="m" val="0.25"/>
	</section>

	<section name="Front Right Wheel">
		<attnum name="ypos" unit="m" val="-0.795"/>
		<attnum name="rim diameter" unit="in" val="15"/>
		<attnum name="tire width" unit="mm" val="304"/>
		<attnum name="tire height-width ratio" val=".54"/>
		<attnum name="inertia" unit="kg.m2" val="1.2200"/>
		
		<!-- initial ride height -->
		<attnum name="ride height" unit="mm" min="70" max="120" val="80"/>
		<attnum name="toe" unit="deg" min="-3" max="3" val="0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-4"/>
		<attnum name="stiffness" min="5.0" max="30.0" val="20.0"/>
		<attnum name="dynamic friction" unit="%" val="90"/>
		<attnum name="mu" val="1.6"/>
	</section>

	<section name="Front Left Wheel">
		<attnum name="ypos" unit="m" val="0.795"/>
		<attnum name="rim diameter" unit="in" val="15"/>
		<attnum name="tire width" unit="mm" val="304"/>
		<attnum name="tire height-width ratio" val=".54"/>
		<attnum name="inertia" unit="kg.m2" val="1.2200"/>

		<!-- initial ride height -->
		<attnum name="ride height" unit="mm" min="70" max="120" val="80"/>
		<attnum name="toe" unit="deg" min="-3" max="3" val="0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-4"/>
		<attnum name="stiffness" min="5.0" max="30.0" val="20.0"/>
		<attnum name="dynamic friction" unit="%" val="90"/>
		<attnum name="mu" val="1.6"/>
	</section>

	<section name="Rear Right Wheel">
		<attnum name="ypos" unit="m" val="-0.795"/>
		<attnum name="rim diameter" unit="in" val="15"/>
		<attnum name="tire width" unit="mm" val="304"/>
		<attnum name="tire height-width ratio" val=".54"/>
		<attnum name="inertia" unit="kg.m2" val="1.2200"/>

		<!-- initial ride height -->
		<attnum name="ride height" unit="mm" min="70" max="120" val="100"/>
		<attnum name="toe" unit="deg" min="-3" max="3" val="0.3"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-3"/>
		<attnum name="stiffness" min="5.0" max="30.0" val="20.0"/>
		<attnum name="dynamic friction" unit="%" val="90"/>
		<attnum name="mu" val="1.6"/>
	</section>

	<section name="Rear Left Wheel">
		<attnum name="ypos" unit="m" val="0.795"/>
		<attnum name="rim diameter" unit="in" val="15"/>
		<attnum name="tire width" unit="mm" val="304"/>
		<attnum name="tire height-width ratio" val=".54"/>
		<attnum name="inertia" unit="kg.m2" val="1.2200"/>

		<!-- initial ride height -->
		<attnum name="ride height" unit="mm" min="70" max="120" val="100"/>
		<attnum name="toe" unit="deg" min="-3" max="3" val="-0.3"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-3"/>
		<attnum name="stiffness" min="5.0" max="30.0" val="20.0"/>
		<attnum name="dynamic friction" unit="%" val="90"/>
		<attnum name="mu" val="1.6"/>
	</section>

	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="1500"/>
	</section>

	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
	</section>

	<section name="Front Right Suspension">
		<attnum name="spring" unit="lbs/in" min="500" max="10000" val="7000"/>
		<attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
		<attnum name="bellcrank" min="1" max="5" val="1"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="lbs/in/s" min="50" max="1000" val="400"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="50" max="1000" val="400"/>
		<attnum name="fast bump" unit="lbs/in/s" min="10" max="200" val="80"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="10" max="200" val="80"/>
	</section>

	<section name="Front Left Suspension">
		<attnum name="spring" unit="lbs/in" min="500" max="10000" val="7000"/>
		<attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
		<attnum name="bellcrank" min="1" max="5" val="1"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="lbs/in/s" min="50" max="1000" val="400"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="50" max="1000" val="400"/>
		<attnum name="fast bump" unit="lbs/in/s" min="10" max="200" val="80"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="10" max="200" val="80"/>
	</section>

	<section name="Rear Right Suspension">
		<attnum name="spring" unit="lbs/in" min="500" max="10000" val="7000"/>
		<attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
		<attnum name="bellcrank" min="1" max="5" val="1"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="lbs/in/s" min="50" max="1000" val="400"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="50" max="1000" val="400"/>
		<attnum name="fast bump" unit="lbs/in/s" min="10" max="200" val="80"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="10" max="200" val="80"/>
	</section>

	<section name="Rear Left Suspension">
		<attnum name="spring" unit="lbs/in" min="500" max="10000" val="7000"/>
		<attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
		<attnum name="bellcrank" min="1" max="5" val="1"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="lbs/in/s" min="50" max="1000" val="400"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="50" max="1000" val="400"/>
		<attnum name="fast bump" unit="lbs/in/s" min="10" max="200" val="80"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="10" max="200" val="80"/>
	</section>

	<section name="Front Right Brake">
		<attnum name="disk diameter" unit="mm" val="380"/>
	</section>

	<section name="Front Left Brake">
		<attnum name="disk diameter" unit="mm" val="380"/>
	</section>

	<section name="Rear Right Brake">
		<attnum name="disk diameter" val="280" unit="mm" min="200" max="350"/>
	</section>

	<section name="Rear Left Brake">
		<attnum name="disk diameter" val="280" unit="mm" min="200" max="350"/>
	</section>
</params>
