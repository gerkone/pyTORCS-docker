  </section>

  <section name="Aerodynamics">
    <attnum name="Cx" val="0.6"/>
    <attnum name="front area" val="1.9"/>
  </section>

  <section name="Rear Wing">
    <attnum name="area" unit="m2" val="1.0"/>
    <attnum name="angle" unit="deg" min="0" max="30" val="0"/>
    <attnum name="xpos" unit="m" val="-1.60"/>
    <attnum name="zpos" unit="m" val=".8"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
        <attnum name="ratio" min="-6" max="-2" val="-4.0"/>
        <attnum name="inertia" val="0.0037"/>
        <attnum name="efficiency" val="0.957"/>
      </section>

      <section name="1">
        <attnum name="ratio" min="0.5" max="5" val="3.5"/>
        <attnum name="inertia" val="0.003"/>
        <attnum name="efficiency" val="0.955"/>
      </section>

      <section name="2">
        <attnum name="ratio" min="0.5" max="5" val="3.0"/>
        <attnum name="inertia" val="0.0037"/>
        <attnum name="efficiency" val="0.957"/>
      </section>

      <section name="3">
        <attnum name="ratio" min="0.5" max="5" val="2.5"/>
        <attnum name="inertia" val="0.0048"/>
        <attnum name="efficiency" val="0.950"/>
      </section>

      <section name="4">
        <attnum name="ratio" min="0.5" max="5" val="2.0"/>
        <attnum name="inertia" val="0.0064"/>
        <attnum name="efficiency" val="0.983"/>
      </section>

      <section name="5">
        <attnum name="ratio" min="0.5" max="5" val="1.5"/>
        <attnum name="inertia" val="0.0107"/>
        <attnum name="efficiency" val="0.948"/>
      </section>

      <section name="6">
        <attnum name="ratio" min="0.5" max="5" val="0.9"/>
        <attnum name="inertia" val="0.0150"/>
        <attnum name="efficiency" val="0.940"/>
      </section>
    </section>
  </section>

  <section name="Steer">
    <attnum name="steer lock" unit="deg" min="9" max="45" val="45"/>
    <attnum name="max steer speed" unit="deg/s" min="1" max="360" val="360"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.65"/>
    <attnum name="max pressure" unit="kPa" min="5000" max="40000" val="5000"/>
  </section>

  <section name="Front Axle">
    <attnum name="xpos" val="1.20"/>
    <attnum name="roll center height" val="0.3"/>
  </section>

  <section name="Rear Axle">
    <attnum name="xpos" val="-1.27"/>
    <attnum name="roll center height" unit="m" val="0.25"/>
  </section>

  <section name="Rear Differential">
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="SPOOL"/>
    <attnum name="ratio" min="1" max="10" val="3.5"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ypos" unit="m" val="-0.9"/>
    <attnum name="rim diameter" unit="in" min="13" max="18" val="15.0"/>
    <attnum name="tire width" unit="mm" min="135" max="200" val="165"/>
    <attnum name="tire height-width ratio" min="0.5" max="1.0" val="0.8"/>
    <attnum name="stiffness" min="5.0" max="20.0" val="15.0"/>

    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="200" max="400" val="300"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="dynamic friction" min="75" max="120" val="100" unit="%"/>
    <attnum name="mu" min="0.05" max="1.8" val="1.5"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ypos" unit="m" val="0.9"/>
    <attnum name="rim diameter" unit="in" min="13" max="18" val="15.0"/>
    <attnum name="tire width" unit="mm" min="135" max="200" val="165"/>
    <attnum name="tire height-width ratio" min="0.5" max="1.0" val="0.8"/>
    <attnum name="stiffness" min="5.0" max="20.0" val="15.0"/>

    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="200" max="400" val="300"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="dynamic friction" min="75" max="120" val="100" unit="%"/>
    <attnum name="mu" min="0.05" max="1.8" val="1.5"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ypos" unit="m" val="-0.9"/>
    <attnum name="rim diameter" unit="in" min="13" max="18" val="15.0"/>
    <attnum name="tire width" unit="mm" min="135" max="200" val="185"/>
    <attnum name="tire height-width ratio" min="0.5" max="1.0" val="0.7"/>
    <attnum name="stiffness" min="5.0" max="20.0" val="15.0"/>

    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="200" max="400" val="300"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="dynamic friction" min="75" max="120" val="100" unit="%"/>
    <attnum name="mu" min="0.05" max="1.8" val="1.5"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ypos" unit="m" val="0.9"/>
    <attnum name="rim diameter" unit="in" min="13" max="18" val="15.0"/>
    <attnum name="tire width" unit="mm" min="135" max="200" val="185"/>
    <attnum name="tire height-width ratio" min="0.5" max="1.0" val="0.7"/>
    <attnum name="stiffness" min="5.0" max="20.0" val="15.0"/>

    <!-- initial ride height -->
    <attnum name="ride height" unit="mm" min="200" max="400" val="300"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="dynamic friction" min="75" max="120" val="100" unit="%"/>
    <attnum name="mu" min="0.05" max="1.8" val="1.5"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="100" max="10000" val="1000"/>
    <attnum name="suspension course" val="0.5"/>
    <attnum name="bellcrank" val="1"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="200" val="50"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="200" val="50"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="100" max="10000" val="1000"/>
    <attnum name="suspension course" val="0.5"/>
    <attnum name="bellcrank" val="1"/>
    <attnum name="slow bump" unit="lbs/in/s" min="20" max="1000" val="50"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="20" max="1000" val="50"/>
    <attnum name="fast bump" unit="lbs/in/s" min="10" max="200" val="50"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="10" max="200" val="50"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="100" max="10000" val="1000"/>
    <attnum name="suspension course" val="0.5"/>
    <attnum name="bellcrank" val="1"/>
    <attnum name="slow bump" unit="lbs/in/s" min="20" max="1000" val="50"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="20" max="1000" val="50"/>
    <attnum name="fast bump" unit="lbs/in/s" min="10" max="200" val="50"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="10" max="200" val="50"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="100" max="10000" val="1000"/>
    <attnum name="suspension course" val="0.5"/>
    <attnum name="bellcrank" val="1"/>
    <attnum name="slow bump" unit="lbs/in/s" min="20" max="1000" val="50"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="20" max="1000" val="50"/>
    <attnum name="fast bump" unit="lbs/in/s" min="10" max="200" val="50"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="10" max="200" val="50"/>
  </section>

  <section name="Front Right Brake">
    <attnum name="disk diameter" unit="mm" val="250"/>
  </section>

  <section name="Front Left Brake">
    <attnum name="disk diameter" unit="mm" val="250"/>
  </section>

  <section name="Rear Right Brake">
    <attnum name="disk diameter" unit="mm" val="100"/>
  </section>

  <section name="Rear Left Brake">
    <attnum name="disk diameter" unit="mm" val="100"/>
  </section>
</params>
