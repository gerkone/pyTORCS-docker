			</section>
			<section name="13">
				<attnum name="rpm" unit="rpm" val="6000"/>
				<attnum name="Tq" unit="N.m" val="420"/>
			</section>
			<section name="14">
				<attnum name="rpm" unit="rpm" val="6500"/>
				<attnum name="Tq" unit="N.m" val="390"/>
			</section>
			<section name="15">
				<attnum name="rpm" unit="rpm" val="7000"/>
				<attnum name="Tq" unit="N.m" val="360"/>
			</section>
			<section name="16">
				<attnum name="rpm" unit="rpm" val="7500"/>
				<attnum name="Tq" unit="N.m" val="350"/>
			</section>
			<section name="17">
				<attnum name="rpm" unit="rpm" val="8000"/>
				<attnum name="Tq" unit="N.m" val="340"/>
			</section>
			<section name="18">
				<attnum name="rpm" unit="rpm" val="8500"/>
				<attnum name="Tq"  unit="N.m" val="320"/>
			</section>
			<section name="19">
				<attnum name="rpm" unit="rpm" val="9000"/>
				<attnum name="Tq" unit="N.m" val="300"/>
			</section>
			<section name="20">
				<attnum name="rpm" unit="rpm" val="9500"/>
				<attnum name="Tq" unit="N.m" val="280"/>
			</section>
			<section name="21">
				<attnum name="rpm" unit="rpm" val="10000"/>
				<attnum name="Tq" unit="N.m" val="250"/>
			</section>
		</section>
	</section>
	
	<section name="Gearbox">
		<attnum name="shift time" unit="s" val="0.20"/>
		<section name="gears">
			<section name="r">
				<attnum name="ratio" min="-6" max="-2" val="-3.4"/>
				<attnum name="efficiency" val="0.9"/>
			</section>
			<section name="1">
				<attnum name="ratio" min="0.5" max="5" val="2.533333"/>
				<attnum name="efficiency" val="1.0"/>
			</section>
			<section name="2">
				<attnum name="ratio" min="0.5" max="5" val="1.650000"/>
				<attnum name="efficiency" val="1.0"/>
			</section>
			<section name="3">
				<attnum name="ratio" min="0.5" max="5" val="1.208333"/>
				<attnum name="efficiency" val="1.0"/>
			</section>
			<section name="4">
				<attnum name="ratio" min="0.5" max="5" val="0.962963"/>
				<attnum name="efficiency" val="1.0"/>
			</section>
			<section name="5">
				<attnum name="ratio" min="0.5" max="5" val="0.766667"/>
				<attnum name="efficiency" val="1.0"/>
			</section>
			<section name="6">
				<attnum name="ratio" min="0.5" max="5" val="0.688454"/>
				<attnum name="efficiency" val="1.0"/>
			</section>
		</section>
	</section>
	
	<section name="Steer">
		<attnum name="steer lock" unit="deg" min="9" max="23" val="23"/>
		<attnum name="max steer speed" unit="deg/s" min="1" max="360" val="360"/>
	</section>
	
	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.6"/>
		<attnum name="max pressure" unit="kPa" min="5000" max="40000" val="15000"/>
	</section>
	
	<section name="Front Axle">
		<attnum name="xpos" val="1.35"/>
		<attnum name="roll center height" unit="m" val="0.18"/>
	</section>
	
	<section name="Rear Axle">
		<attnum name="xpos" val="-1.43"/>
		<attnum name="roll center height" unit="m" val="0.2"/>
	</section>
	
	<section name="Rear Differential">
		<attstr name="type" in="FREE" val="FREE"/>
		<attnum name="ratio" min="1" max="10" val="4.450000"/>
		<attnum name="efficiency" min=".5" max="1.0" val="1.0"/>
	</section>
	
	<section name="Front Right Wheel">
		<attnum name="ypos" unit="m" val="-0.81"/>
		<attnum name="rim diameter" unit="in" val="18.0"/>
		<attnum name="tire width" unit="mm" val="235"/>
		<attnum name="tire height-width ratio" val=".5"/>
		<attnum name="inertia" unit="kg.m2" val="1.1200"/>
		<attnum name="mu" val="1.5"/>
		<!-- initial ride height -->
		<attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
		<attnum name="stiffness" min="20.0" max="50.0" val="20.0"/>
		<attnum name="dynamic friction" unit="%" min="70" max="100" val="80"/>
		<attnum name="toe" unit="deg" min="-1" max="1" val="0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-4"/>
	</section>
	
	<section name="Front Left Wheel">
		<attnum name="ypos" unit="m" val="0.81"/>
		<attnum name="rim diameter" unit="in" val="18.0"/>
		<attnum name="tire width" unit="mm" val="235"/>
		<attnum name="tire height-width ratio" val=".5"/>
		<attnum name="inertia" unit="kg.m2" val="1.1200"/>
		<attnum name="mu" val="1.5"/>
		<attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
		<attnum name="stiffness" min="20.0" max="50.0" val="20.0"/>
		<attnum name="dynamic friction" unit="%" min="70" max="100" val="80"/>
		<attnum name="toe" unit="deg" min="-1" max="1" val="0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-4"/>
	</section>
	
	<section name="Rear Right Wheel">
		<attnum name="ypos" unit="m" val="-0.81"/>
		<attnum name="rim diameter" unit="in" val="18.0"/>
		<attnum name="tire width" unit="mm" val="335"/>
		<attnum name="tire height-width ratio" val=".35"/>
		<attnum name="inertia" unit="kg.m2" val="1.2200"/>
		<attnum name="mu" val="1.5"/>
		<attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
		<attnum name="stiffness" min="20.0" max="50.0" val="20.0"/>
		<attnum name="dynamic friction" unit="%" min="70" max="100" val="80"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-2"/>
		<attnum name="toe" min="-1" max="1" unit="deg" val="0.2"/>
	</section>
	
	<section name="Rear Left Wheel">
		<attnum name="ypos" unit="m" val="0.81"/>
		<attnum name="rim diameter" unit="in" val="18.0"/>
		<attnum name="tire width" unit="mm" val="335"/>
		<attnum name="tire height-width ratio" val=".35"/>
		<attnum name="inertia" unit="kg.m2" val="1.2200"/>
		<attnum name="mu" val="1.5"/>
		<attnum name="ride height" min="100" max="200" unit="mm" val="100"/>
		<attnum name="stiffness" min="20.0" max="50.0" val="20.0"/>
		<attnum name="dynamic friction" unit="%" min="70" max="100" val="80"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-2"/>
		<attnum name="toe" min="-1" max="1" unit="deg" val="-0.2"/>
	</section>
	
	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
	</section>
	
	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
	</section>
	
	<section name="Front Right Suspension">
		<attnum name="spring" unit="lbs/in" min="500" max="10000" val="2000"/>
		<attnum name="suspension course" unit="m" min="0" max="0.35" val="0.35"/>
		<attnum name="bellcrank" min="1" max="5" val="1.5"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="lbs/in/s" min="50" max="1000" val="200"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="50" max="1000" val="200"/>
		<attnum name="fast bump" unit="lbs/in/s" min="10" max="200" val="50"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="10" max="200" val="50"/>
