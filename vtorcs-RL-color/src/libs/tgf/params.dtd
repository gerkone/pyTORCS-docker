<!ELEMENT params (section)+>
<!ATTLIST params
          name CDATA #REQUIRED
	  type CDATA #REQUIRED
	  mode CDATA #IMPLIED
>

<!ELEMENT section (attnum | attstr | section)+>
<!ATTLIST section
          name CDATA #REQUIRED
>

<!ELEMENT attnum EMPTY>
<!ATTLIST attnum 
          name CDATA #REQUIRED
	  min  CDATA #IMPLIED
	  max  CDATA #IMPLIED
	  unit CDATA #IMPLIED
          val  CDATA #REQUIRED
>

<!ELEMENT attstr EMPTY>
<!ATTLIST attstr 
          name CDATA #REQUIRED
	  in   CDATA #IMPLIED
          val  CDATA #REQUIRED
>





