<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Baja Bug" type="template" mode="mw">
  
  <section name="Car">
    <attnum name="initial fuel" unit="l" min="1.0" max="50.0" val="50.0"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-3" max="0" val="-2.0"/>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="8" val="2.8"/>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="8" val="2.2"/>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="8" val="1.6"/>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="8" val="1.2"/>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="8" val="0.95"/>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="8" val="0.8"/>
      </section>
    </section>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.7"/>
    <attnum name="max pressure" unit="kPa" min="100" max="150000" val="22000"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="stiffness" min="5.0" max="20.0" val="10.0"/>
    <attnum name="ride height" unit="mm" min="200" max="800" val="200"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="-2"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="0"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="stiffness" min="5.0" max="20.0" val="10.0"/>
    <attnum name="ride height" unit="mm" min="200" max="800" val="200"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="2"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="0"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="stiffness" min="5.0" max="20.0" val="10.0"/>
    <attnum name="ride height" unit="mm" min="200" max="800" val="200"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="4"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="0"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="stiffness" min="5.0" max="20.0" val="10.0"/>
    <attnum name="ride height" unit="mm" min="200" max="800" val="200"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="-4"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="0"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1000"/>
    <attnum name="suspension course" unit="m" min="0.1" max="0.8" val="0.3"/>
    <attnum name="bellcrank" val="1"/>
    <attnum name="packers" unit="mm" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1000"/>
    <attnum name="suspension course" unit="m" min="0.1" max="0.8" val="0.3"/>
    <attnum name="bellcrank" val="1"/>
    <attnum name="packers" unit="mm" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1000"/>
    <attnum name="suspension course" unit="m" min="0.1" max="0.8" val="0.3"/>
    <attnum name="bellcrank" val="1"/>
    <attnum name="packers" unit="mm" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="1000"/>
    <attnum name="suspension course" unit="m" min="0.1" max="0.8" val="0.3"/>
    <attnum name="bellcrank" val="1"/>
    <attnum name="packers" unit="mm" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
  </section>
  
  <section name="bt private">
    <attnum name="mufactor" val="0.5"/>
  </section>

</params>


