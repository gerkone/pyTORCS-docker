<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<!-- 3D Model copyright by Butch (K/cendra) (see readme.txt) -->

<params name="Alfa Romeo TZ2">
  <section name="Gearbox">
    <section name="gears">
      <section name="r">
        <attnum name="ratio" val="-4.16"/>
      </section>
      <section name="1">
        <attnum name="ratio" val="3.59"/>
      </section>
      <section name="2">
        <attnum name="ratio" val="2.19"/>
      </section>
      <section name="3">
        <attnum name="ratio" val="1.5"/>
      </section>
      <section name="4">
        <attnum name="ratio" val="1.1"/>
      </section>
      <section name="5">
        <attnum name="ratio" val="0.8"/>
      </section>
    </section>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.1" max="0.9" val="0.62"/>
    <attnum name="max pressure" min="100" max="1.5e+06" unit="kPa" val="21000"/>
  </section>

  <section name="Rear Differential">
    <attstr name="type" in="FREE" val="FREE"/>
    <attnum name="ratio" val="4.1"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ride height" min="100" max="2000" unit="mm" val="150"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-2" max="0" unit="deg" val="0"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ride height" min="100" max="2000" unit="mm" val="150"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-2" max="0" unit="deg" val="0"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ride height" min="100" max="2000" unit="mm" val="150"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-2" max="0" unit="deg" val="0"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ride height" min="100" max="2000" unit="mm" val="150"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-2" max="0" unit="deg" val="0"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="0"/>
    <attnum name="suspension course" min="0" max="1" unit="m" val="0.2"/>
    <attnum name="bellcrank" min="0.5" max="5" val="2.5"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="0"/>
    <attnum name="suspension course" min="0" max="1" unit="m" val="0.2"/>
    <attnum name="bellcrank" min="0.5" max="5" val="2.5"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="2000"/>
    <attnum name="suspension course" min="0" max="1" unit="m" val="0.25"/>
    <attnum name="packers" unit="mm" min="0" max="100" val="50"/>
    <attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="100"/>
    <attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="300"/>
    <attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="100"/>
    <attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="100"/>
