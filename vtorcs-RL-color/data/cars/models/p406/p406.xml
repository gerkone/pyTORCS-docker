		<attnum name="camber" min="-5" max="0" unit="deg" val="-2"/>
		<attnum name="stiffness" val="10.0"/>
		<attnum name="dynamic friction" unit="%" val="70"/>
		<attnum name="toe" min="-5" max="5" unit="deg" val="-0.2"/>
	</section>
	
	<section name="Front Anti-Roll Bar">
		<attnum name="spring" min="0" max="5000" unit="lbs/in" val="800"/>
		<attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
		<attnum name="bellcrank" min="1" max="5" val="1"/>
	</section>
	
	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" min="0" max="5000" unit="lbs/in" val="0"/>
		<attnum name="suspension course" min="0" max="0.5" unit="m" val="0.2"/>
		<attnum name="bellcrank" min="1" max="5" val="1"/>
	</section>
	
